; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Control",
"class": "CSVEditor",
"language": "GDScript",
"path": "res://Addons/file-editor/scripts/CSVEditor.gd"
}, {
"base": "Reference",
"class": "FarmPlot",
"language": "GDScript",
"path": "res://FarmPlot.gd"
}, {
"base": "HTTPRequest",
"class": "GoogleTranslateAPI",
"language": "GDScript",
"path": "res://Addons/file-editor/scripts/GoogleTranslatorAPI.gd"
}, {
"base": "Reference",
"class": "Plot",
"language": "GDScript",
"path": "res://Scripts/Plot.gd"
} ]
_global_script_class_icons={
"CSVEditor": "",
"FarmPlot": "",
"GoogleTranslateAPI": "",
"Plot": ""
}

[application]

config/name="Treevolution"
config/use_custom_user_dir=true
config/custom_user_dir_name="Lime Inc/Treevolution"
config/icon="res://Assets/icon.png"
config/auto_accept_quit=false
config/quit_on_go_back=false

[autoload]

Global="*res://Scripts/GLOBAL.gd"
LastOpenedFiles="*res://addons/file-editor/scripts/LastOpenedFiles.gd"

[editor_plugins]

enabled=PoolStringArray( "res://addons/FileSystemView/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg", "res://addons/file-editor/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://Assets/default_env.tres"
